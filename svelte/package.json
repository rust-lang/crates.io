{
  "name": "crates.io-svelte",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:docker": "API_HOST=http://backend:8888 vite dev",
    "dev:live": "vite dev",
    "dev:local": "API_HOST=http://127.0.0.1:8888 vite dev",
    "dev:staging": "API_HOST=https://staging.crates.io vite dev",
    "dev:msw": "VITE_MSW_ENABLED=true vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic"
  },
  "dependencies": {
    "@crates-io/api-client": "workspace:*",
    "@floating-ui/dom": "1.7.4",
    "chart.js": "4.5.1",
    "date-fns": "4.1.0",
    "highlight.js": "11.11.1",
    "pretty-bytes": "7.1.0",
    "semver": "7.7.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "5.0.0",
    "@eslint/compat": "2.0.1",
    "@eslint/js": "9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@poppanator/sveltekit-svg": "6.0.1",
    "@storybook/addon-a11y": "10.2.0",
    "@storybook/addon-docs": "10.2.0",
    "@storybook/addon-svelte-csf": "5.0.10",
    "@storybook/addon-vitest": "10.2.0",
    "@storybook/sveltekit": "10.2.0",
    "@sveltejs/adapter-static": "3.0.10",
    "@sveltejs/kit": "2.50.1",
    "@sveltejs/vite-plugin-svelte": "6.2.4",
    "@types/node": "24.10.9",
    "@types/semver": "7.7.1",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "chromatic": "13.3.5",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prefer-let": "4.0.1",
    "eslint-plugin-storybook": "10.2.0",
    "eslint-plugin-svelte": "3.14.0",
    "globals": "17.1.0",
    "playwright": "1.57.0",
    "prettier": "3.8.1",
    "prettier-plugin-svelte": "3.4.1",
    "storybook": "10.2.0",
    "svelte": "5.48.1",
    "svelte-check": "4.3.5",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "vite": "7.3.1",
    "vite-bundle-analyzer": "1.3.2",
    "@crates-io/msw": "workspace:*",
    "msw": "2.12.7",
    "vitest": "4.0.18",
    "vitest-browser-svelte": "2.0.2"
  },
  "msw": {
    "workerDirectory": [
      "static"
    ]
  }
}
