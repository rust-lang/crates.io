{
  "name": "crates-io",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/rust-lang/crates.io#readme",
  "bugs": {
    "url": "https://github.com/rust-lang/crates.io/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rust-lang/crates.io.git"
  },
  "license": "(MIT OR Apache-2.0)",
  "author": "",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production && node ./script/precompress-assets.mjs",
    "lint:deps": "ember dependency-lint",
    "lint:hbs": "ember-template-lint app",
    "lint:js": "eslint . --cache",
    "precompress": "node ./script/precompress-assets.mjs",
    "prettier:check": "prettier --check package.json '**/*.js' '**/*.mjs' '**/*.ts'",
    "prettier:write": "prettier --write package.json '**/*.js' '**/*.mjs' '**/*.ts'",
    "start": "ember serve",
    "start:docker": "ember serve --proxy http://backend:8888",
    "start:live": "ember serve --proxy https://crates.io",
    "start:local": "ember serve --proxy http://127.0.0.1:8888",
    "start:staging": "ember serve --proxy https://staging-crates-io.herokuapp.com",
    "test": "ember test",
    "e2e": "playwright test",
    "e2e:svelte": "TEST_APP=svelte playwright test"
  },
  "prettier": {
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-ember-template-tag"
    ],
    "arrowParens": "avoid",
    "importOrder": [
      "^(node:)?(assert|async_hooks|buffer|child_process|cluster|console|constants|crypto|dgram|dns|domain|events|fs|http|http2|https|inspector|module|net|os|path|perf_hooks|process|punycode|querystring|readline|repl|stream|string_decoder|timers|tls|trace_events|tty|url|util|v8|vm|zli)",
      "",
      "^(qunit|ember-qunit|@ember/test-helpers|htmlbars-inline-precompile)$",
      "^ember-exam/",
      "",
      "^@(ember|ember-data|glimmer)/",
      "^(ember|ember-data|rsvp)$",
      "^ember-data/",
      "",
      "<THIRD_PARTY_MODULES>",
      "",
      "^crates-io/",
      "",
      "^[.]"
    ],
    "importOrderParserPlugins": [
      "decorators",
      "typescript"
    ],
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "dependencies": {
    "@floating-ui/dom": "1.7.4",
    "@juggle/resize-observer": "3.4.0",
    "@sentry/ember": "10.35.0",
    "chart.js": "4.5.1",
    "date-fns": "4.1.0",
    "highlight.js": "11.11.1",
    "macro-decorators": "0.1.2",
    "mermaid": "11.12.2",
    "pretty-bytes": "7.1.0",
    "semver": "7.7.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@babel/core": "7.28.6",
    "@babel/eslint-parser": "7.28.6",
    "@babel/plugin-proposal-decorators": "7.28.6",
    "@crates-io/msw": "workspace:*",
    "@ember/optional-features": "2.3.0",
    "@ember/render-modifiers": "3.0.0",
    "@ember/string": "4.0.1",
    "@ember/test-helpers": "5.4.1",
    "@ember/test-waiters": "4.1.1",
    "@embroider/compat": "3.9.3",
    "@embroider/core": "3.5.9",
    "@embroider/webpack": "4.1.2",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@glimmer/component": "2.0.0",
    "@glimmer/tracking": "1.1.2",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@mainmatter/ember-api-actions": "0.6.0",
    "@percy/cli": "1.31.8",
    "@percy/ember": "5.0.0",
    "@percy/playwright": "1.0.10",
    "@playwright/test": "1.58.0",
    "@sinonjs/fake-timers": "15.1.0",
    "@types/node": "24.10.9",
    "@types/sinonjs__fake-timers": "15.0.1",
    "@zestia/ember-auto-focus": "5.1.0",
    "broccoli-asset-rev": "3.0.0",
    "broccoli-funnel": "3.0.8",
    "ember-a11y-testing": "7.1.3",
    "ember-auto-import": "2.12.0",
    "ember-cli": "6.10.0",
    "ember-cli-babel": "8.2.0",
    "ember-cli-bundle-analyzer": "1.0.0",
    "ember-cli-dependency-checker": "3.3.3",
    "ember-cli-dependency-lint": "2.0.1",
    "ember-cli-head": "2.0.0",
    "ember-cli-htmlbars": "6.3.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-notifications": "9.1.0",
    "ember-click-outside": "6.1.1",
    "ember-concurrency": "5.1.0",
    "ember-data": "5.8.1",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-error-route": "0.2.0",
    "ember-eslint-parser": "0.5.13",
    "ember-event-helpers": "0.1.1",
    "ember-inflector": "6.0.0",
    "ember-keyboard": "9.0.4",
    "ember-link": "3.5.0",
    "ember-load-initializers": "3.0.1",
    "ember-modifier": "4.2.2",
    "ember-page-title": "9.0.3",
    "ember-qunit": "9.0.4",
    "ember-resolver": "13.1.1",
    "ember-scoped-css": "0.24.3",
    "ember-scoped-css-compat": "10.2.3",
    "ember-source": "6.9.0",
    "ember-svg-jar": "2.7.1",
    "ember-template-imports": "4.3.0",
    "ember-template-lint": "7.9.3",
    "ember-test-selectors": "7.1.0",
    "ember-truth-helpers": "5.0.0",
    "ember-web-app": "5.0.1",
    "ember-window-mock": "1.0.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-ember": "12.7.5",
    "eslint-plugin-ember-concurrency": "0.5.1",
    "eslint-plugin-prefer-let": "4.0.1",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-qunit": "8.2.5",
    "eslint-plugin-qunit-dom": "0.2.0",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "17.1.0",
    "globby": "16.1.0",
    "loader.js": "4.7.0",
    "match-json": "1.3.7",
    "memory-scroll": "2.0.1",
    "micromark": "4.0.2",
    "micromark-extension-gfm": "3.0.0",
    "msw": "2.12.7",
    "playwright-msw": "3.0.1",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.0",
    "postcss-preset-env": "11.1.1",
    "prettier": "3.8.1",
    "prettier-plugin-ember-template-tag": "2.1.2",
    "qunit": "2.25.0",
    "qunit-console-grouper": "0.3.0",
    "qunit-dom": "3.5.0",
    "tracked-built-ins": "4.1.0",
    "typescript": "5.9.3",
    "webpack": "5.104.1"
  },
  "resolutions": {
    "@babel/runtime": "7.28.6",
    "ember-auto-import": "2.12.0",
    "ember-get-config": "2.1.1",
    "ember-inflector": "6.0.0",
    "ember-modifier": "4.2.2",
    "ember-svg-jar>cheerio": "1.0.0-rc.12",
    "rollup": "npm:@rollup/wasm-node",
    "vite@7": "7.3.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "eslint"
      ],
      "ignoreMissing": [
        "@babel/core",
        "postcss"
      ]
    },
    "onlyBuiltDependencies": [
      "@percy/core",
      "esbuild",
      "msw"
    ],
    "ignoredBuiltDependencies": [
      "core-js"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 1 Firefox version",
    "Firefox ESR",
    "last 1 Safari version",
    "last 1 iOS version",
    "last 1 Edge version",
    "last 1 UCAndroid version",
    "last 1 years"
  ],
  "engines": {
    "node": "^24",
    "pnpm": "^10"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "24.13.0"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
