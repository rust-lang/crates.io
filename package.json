{
  "name": "crates-io",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/rust-lang/crates.io#readme",
  "bugs": {
    "url": "https://github.com/rust-lang/crates.io/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rust-lang/crates.io.git"
  },
  "license": "(MIT OR Apache-2.0)",
  "author": "",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production && node ./script/precompress-assets.mjs",
    "lint:deps": "ember dependency-lint",
    "lint:hbs": "ember-template-lint app",
    "lint:js": "eslint . --cache",
    "precompress": "node ./script/precompress-assets.mjs",
    "prettier:check": "prettier --check package.json '**/*.js' '**/*.mjs' '**/*.ts' '**/*.md'",
    "prettier:write": "prettier --write package.json '**/*.js' '**/*.mjs' '**/*.ts' '**/*.md'",
    "start": "ember serve",
    "start:live": "PROXY_BACKEND=https://crates.io ember serve",
    "start:local": "PROXY_BACKEND=http://127.0.0.1:8888 ember serve",
    "start:staging": "PROXY_BACKEND=https://staging-crates-io.herokuapp.com ember serve",
    "test": "ember exam --split=2 --parallel",
    "test-coverage": "COVERAGE=true npm run test && ember coverage-merge && rm -rf coverage_* coverage/coverage-summary.json && nyc report",
    "e2e": "playwright test"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "dependencies": {
    "@juggle/resize-observer": "3.4.0",
    "@sentry/browser": "7.116.0",
    "@sentry/utils": "7.116.0",
    "chart.js": "4.4.3",
    "date-fns": "3.6.0",
    "highlight.js": "11.9.0",
    "macro-decorators": "0.1.2",
    "mermaid": "10.9.1",
    "morgan": "1.10.0",
    "pretty-bytes": "6.1.1",
    "semver": "7.6.2",
    "tracked-toolbox": "2.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.0",
    "@babel/core": "7.24.6",
    "@babel/eslint-parser": "7.24.6",
    "@babel/plugin-proposal-decorators": "7.24.6",
    "@ember/optional-features": "2.1.0",
    "@ember/render-modifiers": "2.1.0",
    "@ember/string": "3.1.1",
    "@ember/test-helpers": "3.3.0",
    "@ember/test-waiters": "3.1.0",
    "@embroider/compat": "3.5.1",
    "@embroider/core": "3.4.10",
    "@embroider/webpack": "4.0.1",
    "@glimmer/component": "1.1.2",
    "@glimmer/tracking": "1.1.2",
    "@mainmatter/ember-api-actions": "0.6.0",
    "@percy/cli": "1.28.7",
    "@percy/ember": "4.2.0",
    "@percy/playwright": "^1.0.5",
    "@playwright/test": "^1.43.1",
    "@sinonjs/fake-timers": "11.2.2",
    "@types/ember": "^4.0.11",
    "@types/node": "^20.13.0",
    "@types/sinonjs__fake-timers": "^8.1.5",
    "@zestia/ember-auto-focus": "5.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-a11y-testing": "6.1.1",
    "ember-auto-import": "2.7.3",
    "ember-cli": "5.8.1",
    "ember-cli-app-version": "6.0.1",
    "ember-cli-babel": "8.2.0",
    "ember-cli-bundle-analyzer": "1.0.0",
    "ember-cli-code-coverage": "3.0.0",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-dependency-lint": "2.0.1",
    "ember-cli-deprecation-workflow": "2.2.0",
    "ember-cli-head": "2.0.0",
    "ember-cli-htmlbars": "6.3.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-mirage": "3.0.3",
    "ember-cli-notifications": "9.0.0",
    "ember-click-outside": "6.1.0",
    "ember-concurrency": "4.0.2",
    "ember-css-modules": "2.1.0",
    "ember-data": "4.12.8",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-error-route": "0.2.0",
    "ember-event-helpers": "0.1.1",
    "ember-exam": "9.0.0",
    "ember-fetch": "8.1.2",
    "ember-keyboard": "8.2.1",
    "ember-link": "3.2.0",
    "ember-load-initializers": "2.1.2",
    "ember-modifier": "4.1.0",
    "ember-page-title": "8.2.3",
    "ember-qunit": "8.0.2",
    "ember-resolver": "11.0.1",
    "ember-router-scroll": "4.1.2",
    "ember-source": "5.8.0",
    "ember-svg-jar": "2.4.9",
    "ember-template-lint": "6.0.0",
    "ember-test-selectors": "6.0.0",
    "ember-tooltips": "3.6.0",
    "ember-truth-helpers": "4.0.3",
    "ember-web-app": "5.0.1",
    "ember-window-mock": "0.9.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-ember": "12.1.1",
    "eslint-plugin-ember-concurrency": "0.5.1",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-qunit": "8.1.1",
    "eslint-plugin-qunit-dom": "0.2.0",
    "eslint-plugin-unicorn": "53.0.0",
    "globby": "14.0.1",
    "http-proxy": "1.18.1",
    "loader.js": "4.7.0",
    "match-json": "1.3.7",
    "miragejs": "0.1.48",
    "normalize.css": "8.0.1",
    "nyc": "15.1.0",
    "postcss-preset-env": "9.5.14",
    "prettier": "3.3.0",
    "qunit": "2.21.0",
    "qunit-console-grouper": "0.3.0",
    "qunit-dom": "3.1.2",
    "tracked-built-ins": "3.3.0",
    "typescript": "^5.4.5",
    "webpack": "5.91.0"
  },
  "resolutions": {
    "ember-get-config": "2.1.1",
    "ember-inflector": "4.0.2",
    "ember-modifier": "4.1.0",
    "miragejs": "0.1.48"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "eslint"
      ],
      "ignoreMissing": [
        "@babel/core",
        "postcss"
      ]
    }
  },
  "engines": {
    "node": "20.14.0",
    "pnpm": "9.1.4"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "20.14.0"
  }
}
