    "typescript": "5.7.3",
    "webpack": "5.97.1"
  },
  "resolutions": {
    "ember-get-config": "2.1.1",
    "ember-inflector": "5.0.2",
    "ember-modifier": "4.2.0",
    "ember-svg-jar>cheerio": "1.0.0-rc.12",
    "miragejs": "0.1.48"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "eslint"
      ],
      "ignoreMissing": [
        "@babel/core",
        "postcss"
      ]
    }
  },
  "engines": {
    "node": "22.13.0",
    "pnpm": "9.15.4"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "22.13.0"
  }
}
