{
  "name": "cargo",
  "version": "0.0.0",
  "homepage": "https://github.com/rust-lang/crates.io#readme",
  "bugs": {
    "url": "https://github.com/rust-lang/crates.io/issues"
  },
  "license": "(MIT OR Apache-2.0)",
  "author": "",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rust-lang/crates.io.git"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint . --cache",
    "lint:deps": "ember dependency-lint",
    "prettier": "prettier --write '{app,tests,mirage}/**/*.js'",
    "start": "ember serve",
    "start:live": "ember serve --proxy https://crates.io",
    "start:local": "ember serve --proxy http://127.0.0.1:8888",
    "start:staging": "ember serve --proxy https://staging-crates-io.herokuapp.com",
    "test": "ember exam --split=2 --parallel",
    "test-coverage": "COVERAGE=true npm run test && ember coverage-merge && rm -rf coverage_* coverage/coverage-summary.json && nyc report"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "broccoli-asset-rev": "2.5.0",
    "ember-a11y-testing": "^0.5.0",
    "ember-cli": "~3.9.0",
    "ember-cli-app-version": "^3.1.3",
    "ember-cli-babel": "^6.14.1",
    "ember-cli-cjs-transform": "^1.3.0",
    "ember-cli-clipboard": "^0.9.0",
    "ember-cli-code-coverage": "^1.0.0-beta.4",
    "ember-cli-dependency-checker": "^2.0.1",
    "ember-cli-dependency-lint": "^1.1.3",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "1.6.1",
    "ember-cli-meta-tags": "^5.3.0",
    "ember-cli-mirage": "^0.4.7",
    "ember-cli-moment-shim": "^3.5.0",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sass": "^7.2.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-template-lint": "^0.7.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-component-attributes": "^0.1.1",
    "ember-concurrency": "^0.8.19",
    "ember-data": "~2.18.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-exam": "^1.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^6.5.1",
    "ember-keyboard": "^4.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-moment": "^7.5.0",
    "ember-page-title": "^4.0.5",
    "ember-percy": "^1.4.0",
    "ember-prism": "^0.2.0",
    "ember-resolver": "^4.1.0",
    "ember-router-scroll": "^0.4.0",
    "ember-source": "~3.1.0",
    "ember-svg-jar": "^1.1.1",
    "ember-test-selectors": "^0.3.7",
    "ember-web-app": "^2.2.0",
    "emberx-select": "^3.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-ember": "^5.1.0",
    "eslint-plugin-prettier": "^2.6.0",
    "loader.js": "^4.5.1",
    "normalize.css": "^7.0.0",
    "nyc": "^11.6.0",
    "prettier": "^1.13.4",
    "qunit-dom": "^0.8.4",
    "timekeeper": "^2.1.0"
  },
  "engines": {
    "node": "^8.1.3",
    "npm": "^5.1.0"
  },
  "private": true,
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all"
  }
