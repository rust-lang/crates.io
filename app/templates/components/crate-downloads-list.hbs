<ul>
    {{#each crates as |crate|}}
        <li>
            {{#link-to 'crate' crate.id class='name'}}
                {{ crate.name }} ({{ crate.max_version }})
                <div class='right'>
                    {{svg-jar "download-clear-back"}}
                    {{ format-num crate.downloads }}
                </div>
            {{/link-to}}
        </li>
    {{/each}}
</ul>
